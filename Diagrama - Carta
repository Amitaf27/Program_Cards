package classes;

public class Carta {
	private String nome, metodo, descricao;
	
	//construtor
	 public Carta() {
		 
	 }

	//get e set
	 public String getNome() {
		return nome;
	 }

	 public void setNome(String nome) {
		this.nome = nome;
	 }

	 public String getMetodo() {
		return metodo;
	 }

	 public void setMetodo(String metodo) {
		this.metodo = metodo;
	 }

	 public String getDescricao() {
		return descricao;
	 }

	 public void setDescricao(String descricao) {
		this.descricao = descricao;
	 }
	 
	 void virar() {
		 
	 }
	 
	 void ler() {
		 
	 }
	 
	 void arrastar() {
		 
	 }
}
